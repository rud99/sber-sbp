{
    "name": "rud99/sber-sbp",
    "description": "Sberbank SBP payments",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikhail Zakharov",
            "email": "debweb18@gmail.com"
        }
    ],
    "homepage": "https://github.com/rud99/sber-sbp",
    "keywords": ["Laravel", "Sber", "Sbp"],
    "minimum-stability": "dev",
    "require": {
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
    },
    "autoload": {
